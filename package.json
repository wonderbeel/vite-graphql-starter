{
  "name": "graphql-starter",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,vue}'",
    "generate:graphql": "graphql-codegen -r dotenv/config --config codegen.yml",
    "test:e2e": "cypress open",
    "test:e2e-run": "cypress run",
    "test:unit": "jest",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@urql/vue": "^0.6.1",
    "@vueuse/core": "^8.2.5",
    "graphql": "^16.0.1",
    "graphql-tag": "^2.12.6",
    "pinia": "^2.0.13",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.2.2",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/typescript": "2.3.1",
    "@graphql-codegen/typescript-operations": "2.2.0",
    "@graphql-codegen/typescript-vue-urql": "^2.2.0",
    "@iconify/json": "^2.1.26",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@unocss/preset-icons": "^0.30.12",
    "@unocss/preset-web-fonts": "^0.30.12",
    "@unocss/reset": "^0.30.12",
    "@vitejs/plugin-vue": "^2.3.1",
    "cypress": "^9.5.3",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.12.0",
    "eslint-plugin-vue": "^8.6.0",
    "jest": "^27.5.1",
    "rollup-plugin-gzip": "^3.0.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "typescript": "^4.6.3",
    "unocss": "^0.30.12",
    "unplugin-auto-import": "^0.6.9",
    "unplugin-icons": "^0.14.1",
    "unplugin-vue-components": "^0.18.5",
    "vite": "^2.9.1",
    "vue-tsc": "^0.33.9"
  }
}
